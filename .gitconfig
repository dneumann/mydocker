[alias]
	lg = log --oneline --all --graph --decorate
	ss = status -s
	ignore = update-index --assume-unchanged
	unignore = update-index --no-assume-unchanged
	ignored = !git ls-files -v | grep "^[[:lower:]]"
[core]
	pager = less
[user]
	name = Dennis Neumann
	email = neumann@sub.uni-goettingen.de
[push]
	default = simple
